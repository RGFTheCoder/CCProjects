	},\
	add = UI.Button {\
		x = -4, y = 4,\
		text = '+',\
		event = 'action',\
		help = 'Install or update',\
	},\
	remove = UI.Button {\
		x = -4, y = 6,\
		text = '-',\
		event = 'action',\
		operation = 'uninstall',\
